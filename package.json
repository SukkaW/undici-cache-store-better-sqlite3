{
  "name": "undici-cache-store-better-sqlite3",
  "version": "0.1.1",
  "description": "A undici cache store powered by `better-sqlite3` instead of `node:sqlite`",
  "repository": {
    "url": "https://github.com/SukkaW/undici-cache-store-better-sqlite3"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.mjs",
  "types": "./dist/cjs/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/es/index.d.mts",
        "default": "./dist/es/index.mjs"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "scripts": {
    "lint": "eslint --format=sukka .",
    "build": "bunchee --minify --runtime nodejs",
    "prerelease": "pnpm run lint && pnpm run build",
    "release": "bumpp -r --all --commit \"release: %s\" --tag \"%s\""
  },
  "keywords": [
    "undici",
    "cache",
    "cache store",
    "sqlite",
    "http cache"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "better-sqlite3": "^11.9.1"
  },
  "devDependencies": {
    "@eslint-sukka/node": "^6.17.1",
    "@types/better-sqlite3": "^7.6.12",
    "@types/node": "^22.13.11",
    "bumpp": "^10.1.0",
    "bunchee": "^6.5.0",
    "eslint": "^9.22.0",
    "eslint-config-sukka": "^6.17.1",
    "eslint-formatter-sukka": "^6.17.1",
    "typescript": "^5.8.2",
    "undici": "^7.5.0"
  },
  "peerDependencies": {
    "undici": ">=7.0.0"
  },
  "packageManager": "pnpm@10.6.5"
}
